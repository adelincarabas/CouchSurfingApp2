<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="613.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RegisterController">
   <children>
      <AnchorPane prefHeight="82.0" prefWidth="520.0" style="-fx-background-color: #E96151;">
         <children>
            <Label layoutX="169.0" layoutY="23.0" text="User Registration" textFill="WHITE">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Label layoutX="92.0" layoutY="92.0" text="Firstname">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="firstnameTextField" layoutX="92.0" layoutY="117.0" prefHeight="25.0" prefWidth="336.0" promptText="Firstname" />
      <Label layoutX="92.0" layoutY="150.0" text="Lastname">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="lastnameTextField" layoutX="92.0" layoutY="175.0" prefHeight="25.0" prefWidth="336.0" promptText="Lastname" />
      <Label layoutX="92.0" layoutY="217.0" text="Username">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" layoutX="92.0" layoutY="242.0" prefHeight="25.0" prefWidth="336.0" promptText="Username" />
      <Label layoutX="92.0" layoutY="282.0" text="Password">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <PasswordField fx:id="setPasswordField" layoutX="92.0" layoutY="307.0" prefHeight="25.0" prefWidth="336.0" promptText="Password" />
      <Label layoutX="95.0" layoutY="346.0" text="Confirm password">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <PasswordField fx:id="confirmPasswordField" layoutX="92.0" layoutY="371.0" prefHeight="25.0" prefWidth="336.0" promptText="Confirm Password" />
      <Button fx:id="registerButton" layoutX="92.0" layoutY="501.0" mnemonicParsing="false" onAction="#registerButtonOnAction" prefHeight="25.0" prefWidth="336.0" style="-fx-background-color: #E96151;" text="Register" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="closeButton" layoutX="92.0" layoutY="547.0" mnemonicParsing="false" onAction="#closeButtonOnAction" prefHeight="25.0" prefWidth="336.0" style="-fx-background-color: #E96151;" text="Close" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label fx:id="registrationMessageLabel" alignment="CENTER" layoutX="14.0" layoutY="92.0" prefHeight="17.0" prefWidth="494.0" textFill="#00c5ff">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label fx:id="confirmPasswordLabel" alignment="CENTER" layoutX="95.0" layoutY="402.0" prefHeight="19.0" prefWidth="330.0" textFill="RED">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <CheckBox fx:id="userbox" layoutX="176.0" layoutY="430.0" mnemonicParsing="false" text="User" />
      <CheckBox fx:id="hostbox" layoutX="288.0" layoutY="430.0" mnemonicParsing="false" text="Host" />
